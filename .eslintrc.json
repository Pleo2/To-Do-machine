{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended" , "airbnb", "prettier", "plugin:testing-library/react"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "testing-library"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "no-undef": "off",
    "no-unused-vars": "warn"
  }
}
